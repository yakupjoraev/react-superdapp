{
  "author": "xbanking",
  "description": "Manage your accounts outside of dapps. Injects the accounts and allows signing transactions for a specific account.",
  "homepage_url": "https:/app.xbanking.org",
  "name": "SUPER Wallet",
  "short_name": "SUPER",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage", "tabs", "scripting", "activeTab"],
  "background": {
    "scripts": ["./assets/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "SUPER",
    "default_popup": "index.html"
  },
  "content_scripts": [{
    "js": ["./src/scripts/content.js"],
    "matches": ["http://*/*", "https://*/*"],
    "run_at": "document_start"
  }],
  "externally_connectable": {
    "web_acessible_resources": ["./src/scripts/injectScript.js", "index.html"],
    "matches": ["http://*/*", "https://*/*"]
  },
  "icons": {
    "16": "./img/favicon/favicon-16x16.png",
    "32": "./img/favicon/favicon-32x32.png",
    "48": "./img/favicon/favicon.ico",
    "128": "./img/favicon/mstile-70x70.png"
  },
  "web_accessible_resources": [
    "*.js",
    "*.css"
  ],
  "content_security_policy": "script-src 'self' blob: 'unsafe-eval' 'wasm-eval'; object-src 'self'"
}